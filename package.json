{
  "name": "landos",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "stylelint": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "htmlValidate": "html-validate dist/**/*.html",
    "format": "prettier --write --plugin-search-dir=. 'src/**/*.{js,ts}'",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "eslint --fix 'src/**/*.{js,ts}'",
    "checkTypes": "tsc --noEmit",
    "lint": "npm-run-all -p eslint checkTypes stylelint htmlValidate",
    "lint:fix": "npm-run-all eslint:fix checkTypes format stylelint:fix",
    "dev": "vite",
    "makeWebp": "node makeWebp.js",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "npm-run-all build preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.5.0",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "autoprefixer": "^10.4.15",
    "css-declaration-sorter": "^7.0.3",
    "eslint": "^8.47.0",
    "html-validate": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-plugin": "^1.0.0",
    "prettier": "^3.0.2",
    "sass": "^1.65.1",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "swiper": "^11.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.5.2",
    "vite-plugin-imagemin": "^0.6.1"
  },
  "dependencies": {
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0"
  }
}
